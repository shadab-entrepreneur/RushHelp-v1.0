<Page
    x:Class="TempApp.secondpage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:TempApp"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid Background="Aqua">

        <Image x:ConnectionId='1' x:Name="Accident"  HorizontalAlignment="Left" Height="163" Margin="10,220,0,0" VerticalAlignment="Top" Width="190" Source="Assets/ACCIDENT.jpg"                      />
        <Image x:ConnectionId='2' x:Name="BloodDonate" HorizontalAlignment="Left" Height="163" Margin="226,220,0,0" VerticalAlignment="Top" Width="164" Source="Assets/Blood Donation.jpg"                         />
        <Image x:ConnectionId='3' x:Name="Medicalhelp" HorizontalAlignment="Left" Height="163" Margin="10,404,0,0" VerticalAlignment="Top" Width="190" Source="Assets/MEDICAL HELP.jpg"                          Stretch="Fill"/>
        <Image x:ConnectionId='4' x:Name="WomenHarrasement" HorizontalAlignment="Left" Height="163" Margin="226,404,0,0" VerticalAlignment="Top" Width="164" Source="Assets/FEMALE HARASSMENT.jpg"                               RenderTransformOrigin="0.458,0.491" Stretch="Fill"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="194" Margin="0,10,0,0" VerticalAlignment="Top" Width="400" Source="Assets/RUSHELP.jpg"/>

    </Grid>
</Page>

